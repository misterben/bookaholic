{
	"name": "Bookaholic Extension",
	"version": "1.0",
	"manifest_version": 2,
	"description": "First attempt at the Bookaholic extension",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
		"tabs",
        "http://*.amazon.co.uk/",
        "http://*.alibris.co.uk/"
	]
}
